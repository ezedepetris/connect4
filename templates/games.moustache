<html>
  <style>
      button{
          font-weight: bold;
          width: 100%;
          margin-top: 28px;
          height: 55px;
          border-radius: 11px;
          font-size: 22px;
          box-shadow: -5px 5px 1px rgba(0, 0, 0, 0.84);
          border: 1px solid #FF0;
          background: none repeat scroll 0% 0% #DF0008;
          color: #FFE600;
          text-shadow: -1px 2px 1px rgba(0, 0, 0, 0.84);
      }
     table {
            position:relative;
            background: #3498db;
            background-image: -webkit-linear-gradient(top, #3498db, #216996);
            background-image: -moz-linear-gradient(top, #3498db, #216996);
            background-image: -ms-linear-gradient(top, #3498db, #216996);
            background-image: -o-linear-gradient(top, #3498db, #216996);
            background-image: linear-gradient(to bottom, #3498db, #216996);
            -webkit-border-radius: 20;
            -moz-border-radius: 20;
            border-radius: 24px;
            font-family: Arial;
            color: #ffffff;
            font-size: 14px;
            padding: 5px 15px 5px 15px;
            text-decoration: none;
        }
        p{
          font-family: Verdana;
          font-size: 30px;
        }
  </style>
  <head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <body>
      <h1>Games</h1>


        <table width="50%" border="3" cellspacing="4" cellpadding="3">  
            <td><h4>Id</h4></td>
            <td><h4>GridId</h4></td>
            <td><h4>Player1</h4></td>
            <td><h4>Player2</h4></td>
            <td><h4>Winner</h4></td>
          </table>
          
        <table width="50%" border="3" cellspacing="3" cellpadding="3">
          {{#games}}
              <tr>
                <td>{{toStringId}}</td>
                <td>{{toStringGridId}}</td>
                <td>{{toStringPlayer1}}</td>
                <td>{{toStringPlayer2}}</td>
                <td>{{toStringWinnerId}}</td>
              </tr>
          {{/games}}
        </table>
    </body>
  </head>
</html>