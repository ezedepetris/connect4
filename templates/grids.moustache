<html>
	<style>
      button{
        font-weight: bold;
        width: 100%;
        margin-top: 28px;
        height: 55px;
        border-radius: 11px;
        font-size: 22px;
        box-shadow: -5px 5px 1px rgba(0, 0, 0, 0.84);
        border: 1px solid #FF0;
        background: none repeat scroll 0% 0% #DF0008;
        color: #FFE600;
        text-shadow: -1px 2px 1px rgba(0, 0, 0, 0.84);
      }
      @font-face { font-family: slkscr; src: url("8-bit/slkscr.ttf"); } 
      h1 {
        color:##FFE600;
        font-family: slkscr;
      }
      h2 {
        color:#FFE600;
        font-family: slkscr;
      }
      h3 {
        color:#FFE600;
        font-family: slkscr;
      }
      h4 {
        color:#FFFFFF;
        font-family: slkscr;
      }
  	</style>
    <head>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
      <body>
      <h1>Grids</h1>


        <table width="50%" border="3" cellspacing="3" cellpadding="3">
          {{#grids}}
              <tr>
                <td><h3>{{toString}}</h3></td>
              </tr>
          {{/grids}}
        </table>
    </body>
  </head>
</html>